    <div id="page-container" style="
        display: flex;
        width: 100vw;
        height: 100vh;
        margin-top: 4em;
        border: 2px black;

      ">

      <%#- JSON.stringify(locals) %>

        <div id="leftsidebar" style="
          display: flex;
					flex-direction: column;
          flex-grow: 100;
          max-width: 15vw;
          margin: 0;">


          <div style="
				display: flex;
				justify-content: center;
				background-color:#28a745;
				margin: 5px;
				padding: 5px;
				border: solid 2px black;
        color:white ;
        font-weight: bold;">

            <a href='/orders/active'>Active Orders</a>

          </div>

          <div style="
				display: flex;
				justify-content: center;
				background-color:#28a745;
				margin: 5px;
				padding: 5px;
				border: solid 2px black;
        color:white ;
        font-weight: bold;">

            <a href='/orders/active'>Oders History</a>

          </div>

          <div style="
          display: flex;
          justify-content: center;
          background-color:#28a745;
          margin: 5px;
          padding: 5px;
          border: solid 2px black;
          color:white ;
          font-weight: bold;">

              <a href='/logout'>logout</a>

            </div>

        </div>




        <div id="content" style="
				 width: 60vw;
				 margin: 0;
				  display: flex;
          justify-content: center;
					flex-wrap: wrap;
					align-content:flex-start">


          <% for( order of items ) { %>
            <div class="foodItem" style="
            border: solid 5px rgb(119, 202, 126);
            padding: 10px;
            margin: 5px;
            min-width: 90%;
             overflow-wrap: break-word;
             ">



              <div class="owner-area">

                  <section class="order-box">

                    <main class="order-details">

                      <div class="set-time">
                        <button class="set-time-button"
                        data-time = "15"
                        >15 MIN</button>
                        <button class="set-time-button"
                        data-time = "30"
                        >30 MIN</button>
                        <button class="set-time-button"
                        data-time = "45"
                        >45 MIN</button>
                      </div>

                      <div class="order-details">
                        <div class="order-number"
                        data-order_no = "ejs expression"
                        >Order#: ejs expression </div>

                        <table class="order-items">
                          <tr class="order-line">
                            <td class="order-qty"> ejs qty</td>
                            <td class="order-item">ejs title</td>
                          </tr>

                          <tr class="order-line">
                            <td class="order-qty">1x</td>
                            <td class="order-item">Salad</td>
                          </tr>

                          <tr class="order-line">
                            <td class="order-qty">2x</td>
                            <td class="order-item">Cheese Cake</td>
                          </tr>
                        </table>

                        <div class="order-notes">Note: Add Raisins</div>
                      </div>

                      <div class="order-fulfilled">
                        <button class="order-fulfilled-button" type="button">
                          <i class="fa-solid fa-check"></i>
                        </button>
                      </div>

                    </main>

                    <footer class="order-time">
                      <div class="time-ordered">12 min ago</div>
                      <div class="time-remaining">20 min remaining</div>
                    </footer>




              </div>




            </div>

            <% } %>

        </div>






