    <div id="page-container" style="
        display: flex;
        width: 100vw;
        height: 100vh;
        margin-top: 4em;
        border: 2px black;

      ">

      <%#- JSON.stringify(locals) %>

      <div id="leftsidebar bg-dark" style="
      display: flex;
      flex-direction: column;
      flex-grow: 100;
      min-width: 150px;
      max-width: 250px;
      margin: 0;
      /* background-color: #343a40; */
      background-color: #343a40;
      flex-wrap: wrap;
      ">


        <div style="
    display: flex;
    justify-content: center;
    /* background-color:#28a745; */
    background-color: #343a40;
    margin: 5px;
    padding: 5px;
    /* border: solid 2px black; */
    color:white ;
    font-weight: bold;
    ">

            <a href='/orders/active' class="btn btn-block btn-dark" role="button">
              Active Orders </a>

          </div>

          <div style="
          display: flex;
          justify-content: center;
          /* background-color:#28a745; */
          margin: 5px;
          padding: 5px;
          /* border: solid 2px black; */
          background-color: #343a40;
          color:white ;
          font-weight: bold;">

                  <a href='/orders/history' class="btn btn-block btn-dark" role="button">
                    Orders History</a>

                </div>


        </div>




        <div id="content" style="
				 width: 60vw;
				 margin: 0;
				  display: flex;
          justify-content: center;
					flex-wrap: wrap;
					align-content:flex-start">


          <% for( order of items ) { %>

            <div class="foodItem" style="
            /* border: solid 0px rgb(119, 202, 126); */
            padding: 10px;
            margin: 5px;
            min-width: 90%;
             overflow-wrap: break-word;
             ">



              <!-- <div class="owner-area"> -->

              <section class="order-box">
                <main class="order-details">

                  <div class="set-time " style="display: flex;
                    flex-direction: column;
                    height: 100%;
                    flex-flow: column;">

                    <!-- {
          id: 1,
          customer_id: 1,
          order_no: "05b9f945-1",
          order_time: "2021-10-21T02:35:47.464Z",
          order_note: " saasfaf ",
          estimated_time: null,
          completed_time: null,
          completed: false
          }, -->


                    <button type="button" class="set-time-button btn btn-outline-dark"
                    data-order_no="<%=   order.order_no %>"
                    data-estimated_time="15">15
                      MIN</button>
                    <button type="button" class="set-time-button btn btn-outline-dark"
                    data-order_no="<%=   order.order_no %>"
                    data-estimated_time="30">30
                      MIN</button>
                    <button type="button" class="set-time-button btn btn-outline-dark"
                    data-order_no="<%=   order.order_no %>"
                    data-estimated_time="45">45
                      MIN</button>
                  </div>

                  <div class="order-items">
                    <div class="order-number" data-order_no="<%=   order.order_no %>" style="text-align: center;">
                      <b>Order#: [ <%= order.id%>%> ]</b></div>

                    <div class="order-line" data-order_no="<%=   order.order_no %>">
                      <%= order.order_no %>
                    </div>
                    <div class="<%=   order.order_no %>">
                      <%= order.order_no %>
                    </div>
                  </div>

                  <div class="order-fulfilled">

                    <button type="button" class="order-fulfilled-button btn btn-outline-success"
                    data-orderNum= <%= order.order_no %>
                    data-completed="false"><i class="fa-solid fa-check"></i></button>

                  </div>

                </main>

                <footer class="order-time">
                  <div class="time-ordered" data-order_time="<%=  order.order_time %>">
                    <%= (order.order_time) %>
                  </div>
                  <div class="time-remaining"><span id="time"></span> min remaining</div>
                </footer>


              </section>





              <!-- </div> -->

            </div>

            <% } %>

        </div>






